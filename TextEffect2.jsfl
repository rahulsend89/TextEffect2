function check(){	my_doc = fl.getDocumentDOM();	if(my_doc.selection.length != 1){		return "tooMany";	} else if(my_doc.selection[0].elementType == "text"){		return "text";	} else if(my_doc.selection[0].name == "holder_mc"){		return "edit";	} else {		return "fail";	}}function setup() {	myText=fl.getDocumentDOM().selection[0].getTextString(0,fl.getDocumentDOM().selection[0].length);	my_doc=fl.getDocumentDOM();	my_doc.selection[0].name = "txtLetter";	my_doc.selection[0].setTextString("m");	my_doc.selection[0].autoExpand = true;	my_doc.selection[0].embeddedCharacters = myText;	my_doc.convertToSymbol("movie clip","letter","top left");	my_doc.library.items[0].linkageExportForAS=true;	my_doc.library.items[0].linkageExportInFirstFrame = true;	my_doc.selectAll();	my_doc.deleteSelection();	//my_doc.selection[0].name="holder_mc";	my_doc.library.addNewItem("movie clip","holder_mc","top left");	my_doc.library.addItemToDocument({x:100, y:100}, "holder_mc");	my_doc.selection[0].name = "holder_mc";	my_doc.selection[0].setPersistentData("waved", "string", "true");	return myText;}function addScript() {	my_doc=fl.getDocumentDOM();	var my_frame=my_doc.getTimeline().layers[0].frames[0];	my_frame.actionScript+= "var myTextEffect:TextEffect = new TextEffect(holder_mc, msg, \"fadeIn\", Number(startX), Number(startY), Number(startXscale), Number(startYscale), Number(startRotation), Number(startAlpha), textFormat, widthLimit, Number(startSpeed), easeEff)";	//var myTextEffect:TextEffect = new TextEffect(holder_mc, "this is a sample text", "fadeIn", Number(startX), Number(startY), Number(startXscale), Number(startYscale), Number(startRotation), Number(startAlpha), textFormat, widthLimit, Number(startSpeed), startC_cb.value);}